<?xml version="1.0"?>
<configuration>

  <configSections>
    <section name="Netlog" type="Nistec.Logging.NetlogConfig, Nistec.Logger, Version=4.0.2.0, Culture=neutral, PublicKeyToken=734a739868a76423" requirePermission="false"/>
    <section name="RemoteCache" type="System.Configuration.IgnoreSectionHandler, System.Configuration, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b03f5f7f11d50a3a" requirePermission="false"/>
  </configSections>
    

  <Netlog>
    <NetlogSettings>
      <add key="LogLevel" value="Debug|Info|Warn|Error|Trace"/>
      <add key="LogMode" value="Console"/>
    </NetlogSettings>
  </Netlog>

  <RemoteCache>

    <CacheApiSettings>

      <add key="RemoteBundleHostName" value="nistec_cache_bundle"/>
      
      <add key="RemoteCacheHostName" value="nistec_cache_bundle"/>
      <add key="RemoteSyncCacheHostName" value="nistec_cache_bundle"/>
      <add key="RemoteSessionHostName" value="nistec_cache_bundle"/>
      <add key="RemoteDataCacheHostName" value="nistec_cache_bundle"/>
      <add key="RemoteCacheManagerHostName" value="nistec_cache_manager"/>
      <add key="IsRemoteAsync" value="false"/>
      <add key="EnableRemoteException" value="true"/>

    </CacheApiSettings>

    <TcpClientSettings>

      <host HostName="nistec_cache_bundle" Address="localhost" Port="13000" IsAsync="false" SendTimeout="5000000" ReadTimeout="1000000" ReceiveBufferSize="4096" SendBufferSize="4096"/>

      <host HostName="nistec_cache" Address="localhost" Port="13000" IsAsync="false" SendTimeout="5000" ReadTimeout="1000" ReceiveBufferSize="4096" SendBufferSize="4096"/>

      <host HostName="nistec_cache_data" Address="localhost" Port="13000" IsAsync="false" SendTimeout="5000" ReadTimeout="1000" ReceiveBufferSize="4096" SendBufferSize="4096"/>

      <host HostName="nistec_cache_session" Address="localhost" Port="13000" IsAsync="false" SendTimeout="5000" ReadTimeout="1000" ReceiveBufferSize="4096" SendBufferSize="4096"/>

      <host HostName="nistec_cache_sync" Address="localhost" Port="13000" IsAsync="false" SendTimeout="5000000" ReadTimeout="1000000" ReceiveBufferSize="4096" SendBufferSize="4096"/>

      <host HostName="nistec_cache_manager" Address="localhost" Port="13005" IsAsync="false" SendTimeout="5000" ReadTimeout="1000" ReceiveBufferSize="4096" SendBufferSize="4096"/>


    </TcpClientSettings>

    <!--PipeDirection="In|Out|InOut"  PipeOptions="None|WriteThrough|Asynchronous"-->
    <PipeClientSettings>
      <pipe PipeName="nistec_cache" PipeDirection="InOut" PipeOptions="None" VerifyPipe="nistec_cache" ConnectTimeout="5000" InBufferSize="8192" OutBufferSize="8192"/>

      <pipe PipeName="nistec_cache_manager" PipeDirection="InOut" PipeOptions="None" VerifyPipe="nistec_cache" ConnectTimeout="5000" InBufferSize="8192" OutBufferSize="8192"/>

      <pipe PipeName="nistec_cache_data" PipeDirection="InOut" PipeOptions="None" VerifyPipe="nistec_cache" ConnectTimeout="5000" InBufferSize="8192" OutBufferSize="8192"/>

      <pipe PipeName="nistec_cache_session" PipeDirection="InOut" PipeOptions="None" VerifyPipe="nistec_cache" ConnectTimeout="5000" InBufferSize="8192" OutBufferSize="8192"/>

    </PipeClientSettings>

  </RemoteCache>
  
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1"/>
  </startup>
</configuration>
